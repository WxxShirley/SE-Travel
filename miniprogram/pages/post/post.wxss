/* miniprogram/pages/post/post.wxss */

.flex-container {
  padding: 5rpx;
  background-color: #fff;
  
 
  margin-left: 10px;
  margin-right: 10px;
}

.empty {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.empty-icon {
  margin-top: 400rpx;
  width: 110rpx;
  height: 110rpx;
}

.empty-text {
  margin-top: 24rpx;
  font-size: 32rpx;
  color: #98989c;
}
.van-swipe-cell__left,
.van-swipe-cell__right {
  display: inline-block;
  width: 65px;
  height: 44px;
  padding-top: 45px;
  font-size: 15px;
  line-height: 44px;
  color: #fff;
  text-align: center;
  background-color: #f44;
  vertical-align: center;
}
